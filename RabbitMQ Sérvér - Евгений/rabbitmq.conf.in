loopback_users = none
log.console = true
log.console.level = debug
log.file.level = debug
listeners.tcp.default = 5672
management.tcp.port = 15672
listeners.ssl.default = 5671
management.ssl.port = 15671
management.ssl.cacertfile = @@DIR@@/certs/ca_certificate.pem
management.ssl.certfile   = @@DIR@@/certs/server_localhost_certificate.pem
management.ssl.keyfile    = @@DIR@@/certs/server_localhost_key.pem
ssl_options.cacertfile    = @@DIR@@/certs/ca_certificate.pem
ssl_options.certfile      = @@DIR@@/certs/server_localhost_certificate.pem
ssl_options.keyfile       = @@DIR@@/certs/server_localhost_key.pem
